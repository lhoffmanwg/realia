
<h2>Edit Agent Details</h2>

<%= form_for(@agent) do |f| %>

    <p><label>Agency:</label>
    <%= f.text_field :agency %></p>

    <p><label>Years of Experience:</label>
    <%= f.text_field :years_exp %></p>

    <p><label>Name:</label>
    <%= f.text_field :name %></p>

    <p><label>Email:</label></p>
    <%= f.email_field :email %>

    <p><label>Phone:</label></p>
    <%= f.text_field :phone  %>

    <%= f.submit %>

<% end %>
