
<h2>Edit Agent Details</h2>

<%= form_for(@agent) do |f| %>

    <p><label>Agency:</label>
    <%= f.text_field :agency_id %></p>

    <p><label>Years of Experience:</label>
    <%= f.text_field :years_exp %></p>

    <p><label>Name:</label>
    <%= f.text_field :name %></p>

    <p><label>Email:</label>
    <%= f.email_field :email %></p>

    <p><label>Phone:</label>
    <%= f.text_field :phone  %></p>

    <%= f.submit %>

<% end %>
